<?xml version="1.0" encoding="utf-8"?>
<!--The outline for all the TextViews in the QU Tracker app-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <stroke
        android:width="2dp"
        android:color="@color/QUnavy"/>
    <padding
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp"/>
    <corners android:radius="5dp"/>
</shape>